{
  "extends": "airbnb/legacy",
  "rules": {
    "semi": [2, "never"],
	  "func-names": 0,
	  "consistent-return" : 0,
	  "no-trailing-spaces" : 1,
    "no-multiple-empty-lines" : ["error", { "max": 1 }]
  },
  "globals": {
    "angular": true,
    "window": true,
    "console": true,
    "debugger": true
  }
}
